{
    "version": 2,
    "builds": [
      { "src": "api/*.py", "use": "@vercel/python" },
      { "src": "pages/**/*", "use": "@vercel/next" }
    ],
    "routes": [
      { "src": "/api/translate", "dest": "/api/translate.py" },
      { "src": "/api/download", "dest": "/api/download.py" }
    ]
  }
  